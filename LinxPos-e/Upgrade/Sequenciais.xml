		<tamanho>8</tamanho>
	</sequenciais>
	<sequenciais>
		<tabela_coluna>LOJA_CAIXA_LOTE.LOTE_REMESSA</tabela_coluna>
		<descricao>NÚMERO DO LOTE DA REMESSA</descricao>
		<sequencia>0000000000</sequencia>
		<tamanho>10</tamanho>
	</sequenciais>
</VFPData>